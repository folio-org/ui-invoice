    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-to-print": "^2.12.4",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.0.0"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "~5.0.0",
